modules = ["nodejs-18", "web", "bash"]
hidden = [".config", "package-lock.json"]
run = "npm run dev"

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_05"
packages = ["bash"]

[deployment]
run = ["sh", "-c", "npm run dev"]
deploymentTarget = "autoscale"
ignorePorts = false

[workflows]
